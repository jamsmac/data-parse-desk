# Supabase Configuration
# Получите эти значения из панели Supabase: https://app.supabase.com/project/_/settings/api

# Project ID (из URL проекта)
VITE_SUPABASE_PROJECT_ID=your_project_id_here

# Anon/Public Key (из Settings > API)
VITE_SUPABASE_PUBLISHABLE_KEY=your_anon_key_here

# Project URL (из Settings > API)
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Sentry Configuration (опционально)
# Получите DSN из Sentry: https://sentry.io/settings/projects/

# Sentry DSN для мониторинга ошибок
VITE_SENTRY_DSN=your_sentry_dsn_here

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Release Version (используется для отслеживания в Sentry)
VITE_APP_VERSION=1.0.0

# Sample Rate для Sentry (0.0 - 1.0)
VITE_SENTRY_SAMPLE_RATE=1.0

# Traces Sample Rate для производительности (0.0 - 1.0)
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
