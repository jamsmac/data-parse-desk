# Supabase Configuration
# Получите эти значения из панели Supabase: https://app.supabase.com/project/_/settings/api

# Project ID (из URL проекта)
VITE_SUPABASE_PROJECT_ID=your_project_id_here

# Anon/Public Key (из Settings > API)
VITE_SUPABASE_PUBLISHABLE_KEY=your_anon_key_here

# Project URL (из Settings > API)
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Sentry Configuration (опционально)
# Получите DSN из Sentry: https://sentry.io/settings/projects/

# Sentry DSN для мониторинга ошибок
VITE_SENTRY_DSN=your_sentry_dsn_here

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Release Version (используется для отслеживания в Sentry)
VITE_APP_VERSION=1.0.0

# Sample Rate для Sentry (0.0 - 1.0)
VITE_SENTRY_SAMPLE_RATE=1.0

# Traces Sample Rate для производительности (0.0 - 1.0)
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1

# Firebase Configuration (для Push-уведомлений)
# Получите эти значения из Firebase Console: https://console.firebase.google.com/
# Project Settings > General > Your apps > Web app

# Firebase API Key
VITE_FIREBASE_API_KEY=your_firebase_api_key_here

# Firebase Auth Domain
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Firebase Project ID
VITE_FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# Firebase Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here

# Firebase App ID
VITE_FIREBASE_APP_ID=your_app_id_here

# Firebase Measurement ID (опционально)
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Firebase VAPID Key (для Web Push)
# Получите из Project Settings > Cloud Messaging > Web configuration > Web Push certificates
VITE_FIREBASE_VAPID_KEY=your_vapid_key_here
